<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zhuxiaoyue.bodytraveling">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="身体奇遇记"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity
            android:name=".Activity.MainActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.HomeActivity" />
        <activity android:name=".Activity.LevelActivity" />
        <activity android:name=".Activity.SystemsActivity" />
        <activity android:name=".Activity.KnowledgeActivity" />
        <activity android:name=".Activity.InLevelActivity"></activity>
    </application>

</manifest>